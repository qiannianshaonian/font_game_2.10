{
  "name": "chrome-simple-launcher",
  "version": "0.1.4",
  "description": "A simple chrome launcher",
  "files": [
    "build"
  ],
  "main": "build/index.js",
  "author": "dustyface<dustyface@163.com",
  "license": "MIT",
  "scripts": {
    "start": "tsc -w",
    "clean": "rimraf build",
    "format": "prettier \"{examples,lib,script,test}/**/*.{js,ts}\" \"*.yml\" --write",
    "test": "jest --detectOpenHandles",
    "build": "npm run clean && tsc -p ./tsconfig.json"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/?(*.)(spec|test).(ts|tsx|js)",
      "**/__tests__/*.(ts|tsx|js)",
      "**/test/*.(ts|tsx|js)"
    ]
  },
  "dependencies": {
    "chrome-paths": "^1.0.1"
  },
  "devDependencies": {
    "@types/jest": "23.3.11",
    "@types/node": "10.12.18",
    "jest": "23.6.0",
    "prettier": "1.15.3",
    "rimraf": "2.6.3",
    "ts-jest": "23.10.5",
    "ts-node": "6.0.0",
    "typescript": "3.2.4"
  },
  "gitHead": "02f23c979a69c03a76aff6fef84ea916b50be82d"
}
